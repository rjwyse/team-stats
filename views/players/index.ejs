<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Player</th>
    </tr>
  </thead>
  <tbody>
    <% players.forEach(function(p) { %>
      <tr>
        <td><%= p.name %></td>
        <td><a href="/players/<%= p._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
